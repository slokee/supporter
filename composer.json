{
    "name": "slokee/supporter",
    "description": "Laravel Supporter is a powerful utility package designed to enhance development speed by providing ready-to-use code snippets. This package helps developers streamline their workflow by reducing repetitive coding tasks, ensuring cleaner and more efficient Laravel applications.",
    "type": "library",
    "minimum-stability": "stable",
    "config": {
        "sort-packages": true
    },
    "keywords": [
        "laravel",
        "supporter",
        "helpers",
        "validation rules",
        "scopes",
        "utilities",
        "code snippets"
    ],
    "require": {
        "php": ">=8.0",
        "laravel/framework": "^11.0 || ^12.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Slokee\\Supporter\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Nilkanth Borad",
            "email": "nilkanthb@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Slokee\\Supporter\\SupportServiceProvider"
            ]
        }
    },
    "require-dev": {
        "laravel/pint": "^1.22"
    }
}
